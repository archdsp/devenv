    name = jisu choi
    signingKey = 9DF07E25EDDD1277F38B52954534AF151D8D70F5
[credential]
    helpergit = credential
    helper = store
    username = jisuchoi01
    useHttpPath = true
[core]
    editor = vim
    autocrlf = input
    notesRef = true
[alias]
    lga = log --graph --decorate=full --notes --oneline --all --show-signature
    lg = log -n5 --all --graph --pretty="%C(reverse)%C(yellow)%h%C(cyan)%d%C(blue)[%G?]%C(white)%s%n%C(white)%C(dim)%cr%ce"
[init]
    templateDir = ~/.git-templates
[rerere]
    enabled = true
[commit]
    gpgsign = true
    template = ~/.gitmessage
[gpg]
    program = /usr/bin/gpg
